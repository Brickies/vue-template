    "js-cookie": "^2.2.1",
    "lint-staged": "^9.4.3",
    "lodash": "^4.17.15",
    "node-sass": "^4.12.0",
    "qs": "^6.9.1",
    "sass-loader": "^8.0.0",{{#stylelint}}

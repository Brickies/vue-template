    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2",
    "vuex-class": "^0.3.2"
  },
  "devDependencies": {
    "@bfe/site-update-checker": "0.0.5",
    "@bfe/snf-lx": "^0.3.0",{{#commitlint}}
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",{{/commitlint}}
    "@ss/mtd-vue": "^0.1.45",
    "@types/js-cookie": "^2.2.4",
    "@types/lodash": "^4.14.149",
